{% extends 'base.html.twig' %}

{% block title %}Hello ListMovieController!{% endblock %}

{% block body %}


<h1>Movies</h1>
<ul class="movies">
    {% for movie in movies %}
        <li><a href="{{ path('app_show_movie', {id: movie.id}) }}">{{ movie.title|e }}</a></li>
    {% endfor %}
</ul>
{% endblock %}
